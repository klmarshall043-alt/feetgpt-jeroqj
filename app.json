
{
  "expo": {
    "name": "FeetGPT",
    "slug": "FeetGPT",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/79afa96e-51b5-4b68-bf0b-5916da7e5df1.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/79afa96e-51b5-4b68-bf0b-5916da7e5df1.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.feetgpt.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/79afa96e-51b5-4b68-bf0b-5916da7e5df1.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.feetgpt.app"
    },
    "web": {
      "favicon": "./assets/images/79afa96e-51b5-4b68-bf0b-5916da7e5df1.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "feetgpt",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  }
}
